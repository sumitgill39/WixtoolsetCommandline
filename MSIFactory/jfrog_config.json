{
    "jfrog_url": "https://artifactory.example.com/artifactory",
    "username": "your_username",
    "password": "your_password",
    "polling_interval": 60,
    "max_threads": 10,
    "download_timeout": 300,
    "retry_attempts": 3,
    "repositories": {
        "snapshots": "libs-snapshot-local",
        "releases": "libs-release-local",
        "feature_builds": "feature-builds-local",
        "release_candidates": "release-candidates-local",
        "hotfixes": "hotfix-builds-local"
    },
    "artifact_patterns": {
        "zip_pattern": "*.zip",
        "version_pattern": "(\\d+\\.\\d+\\.\\d+)",
        "branch_separator": "-"
    }
}